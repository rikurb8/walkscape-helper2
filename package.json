{
  "name": "walkscape-wiki-scraper",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript scraper for WalkScape wiki pages",
  "type": "module",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "tsc -p tsconfig.json",
